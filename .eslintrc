{
  "env": {
    "browser": true,
    "jest/globals": true
  },
  "plugins": [
    "jest"
  ],
  "parserOptions": {
    "sourceType": "module"
  },
  "extends": "airbnb/base",
  "parser": "babel-eslint",
  "rules": {
    "semi": ["error", "never"],
    "no-confusing-arrow": 0,
    "no-nested-ternary": 0,
    "arrow-parens": ["error", "as-needed"]
  }
}
